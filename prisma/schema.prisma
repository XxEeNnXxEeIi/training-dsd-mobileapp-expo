datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Member {
  id        String  @id @default(auto()) @map("_id") @db.ObjectId
  userName  String
  firstName String
  lastName  String
  password  String
  email     String
}

